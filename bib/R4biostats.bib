  title = {Directed Acyclic Graphs: A Tool for Causal Studies in Paediatrics},
  shorttitle = {Directed Acyclic Graphs},
  author = {Williams, Thomas C. and Bach, Cathrine C. and Matthiesen, Niels B. and Henriksen, Tine B. and Gagliardi, Luigi},
  date = {2018-10},
  journaltitle = {Pediatric Research},
